# DStack VPN Environment Configuration
# Copy this file to .env and fill in your values

# WireGuard Configuration
WIREGUARD_PRIVATE_KEY_A=your_wireguard_private_key_for_node_a_here
WIREGUARD_PRIVATE_KEY_B=your_wireguard_private_key_for_node_b_here
WIREGUARD_PRIVATE_KEY_C=your_wireguard_private_key_for_node_c_here

# Hub Configuration (DigitalOcean NYC)
HUB_PUBLIC_IP=your_digitalocean_hub_ip_here
HUB_PUBLIC_KEY=your_hub_public_key_here

# Node Configuration
NODE_A_IP=10.88.0.11
NODE_B_IP=10.88.0.12
NODE_C_IP=10.88.0.13

# Health Check Configuration
HEALTH_CHECK_PORT=8000

# Optional: Custom Path Configuration
# WIREGUARD_PRIVATE_KEY_PATH=/etc/wireguard/private.key 