# DStack VPN Environment Configuration
# Copy this file to .env and fill in your values

# Contract Configuration
CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
RPC_URL=https://mainnet.base.org

# Node A Configuration
NODE_ID_A=node-a
CONTRACT_PRIVATE_KEY_A=your_contract_private_key_for_node_a_here
WIREGUARD_PRIVATE_KEY_A=your_wireguard_private_key_for_node_a_here

# Node B Configuration  
NODE_ID_B=node-b
CONTRACT_PRIVATE_KEY_B=your_contract_private_key_for_node_b_here
WIREGUARD_PRIVATE_KEY_B=your_wireguard_private_key_for_node_b_here

# Network Configuration
NETWORK=base
SYNC_INTERVAL=30000
LOG_LEVEL=info

# Health Check Configuration
HEALTH_CHECK_PORT=8080

# Proxy Configuration
PROXY_PORT=1080

# Optional: Custom RPC endpoints
# BASE_RPC_URL=https://mainnet.base.org
# POLYGON_RPC_URL=https://polygon-rpc.com
# ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc 